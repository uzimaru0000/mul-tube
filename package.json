{
  "name": "mul-tube",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "preview": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron ./dist/electron/index.js",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none ELECTRON=true yarn dev\" \"yarn electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "tsc && cross-env ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn build:electron && yarn electron:builder"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-youtube": "^7.14.0",
    "yoga-layout-wasm": "^1.9.3-alpha.7"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/youtube-player": "^5.5.5",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "typescript": "^4.4.4",
    "vercel": "^24.0.0",
    "vite": "^2.7.2",
    "wait-on": "^6.0.0"
  },
  "build": {
    "productName": "MutTube",
    "appId": "com.uzimaru.mut-tube",
    "extraMetadata": {
      "main": "./dist/electron/index.js"
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ]
  }
}
